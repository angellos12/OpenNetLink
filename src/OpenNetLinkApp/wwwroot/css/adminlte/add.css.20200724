



/* ID S */

	.username {
		background: url('/images/adminlte/login_icon1.png') transparent no-repeat 15px center;
		background-size: 28px auto;
	}

	@-webkit-keyframes username {
		to {
			background: url('/images/adminlte/login_icon1.png') transparent no-repeat 15px center;
			background-size: 28px auto;
		}
	}

	input.username:-webkit-autofill {
		-webkit-animation-name: username;
		-webkit-animation-fill-mode: both;
	}

	/* ./ input ID End */



	/* Password S */

.password {
	background: url('/images/adminlte/login_icon2.png') transparent no-repeat 15px center;
	background-size: 28px auto;
}

	@-webkit-keyframes password {
		to {
			background: url('/images/adminlte/login_icon2.png') transparent no-repeat 15px center;
			background-size: 28px auto;
		}
	}

	input.password:-webkit-autofill {
		-webkit-animation-name: password;
		-webkit-animation-fill-mode: both;
	}

	/* ./ Password End */






