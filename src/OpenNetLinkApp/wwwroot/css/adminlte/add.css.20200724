/* form-control-login */

.form-control-login {
  display: block;
  width: 100%;
  height: calc(2.5rem + 2px);
  padding: 0.375rem 1rem;
  font-size: 1.1rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: #ffffff;
  background-clip: padding-box;
  border: 1px solid #ced4da;
  border-right:0;
  border-radius: 1.5rem;
  box-shadow: inset 0 0 0 rgba(0, 0, 0, 0);
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}

 .form-control-login {
    transition: none;
  }
}

.form-control-login::-ms-expand {
  background-color: transparent;
  border: 0;
}

.form-control-login:-moz-focusring {
  color: transparent;
  text-shadow: 0 0 0 #495057;
}

.form-control-login:focus {
  color: #495057;
  background-color: #ffffff;
  border-color: #80bdff;
  outline: 0;
  box-shadow: inset 0 0 0 rgba(0, 0, 0, 0), none;
}

.form-control-login::-webkit-input-placeholder {
  color: #939ba2;
  opacity: 1;
}

.form-control-login::-moz-placeholder {
  color: #939ba2;
  opacity: 1;
}

.form-control-login:-ms-input-placeholder {
  color: #939ba2;
  opacity: 1;
}

.form-control-login::-ms-input-placeholder {
  color: #939ba2;
  opacity: 1;
}

.form-control-login::placeholder {
  color: #939ba2;
  opacity: 1;
}

.form-control-login:disabled, .form-control-login[readonly] {
 /*background-color: #e9ecef;*/
  background-color: #fff;
  opacity: 1;
}

select.form-control-login:focus::-ms-value {
  color: #495057;
  background-color: #ffffff;
}

.form-control-login-file,
.form-control-login-range {
  display: block;
  width: 100%;
}



/* .input-group-text-login */

.input-group-text-login {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  padding: 0.375rem 1.25rem 0.375rem 0.375rem;
  margin-bottom: 0;
  font-size: .9rem;
  font-weight: 700;
  line-height: 1.5;
  /*color: #495057;
  color: #212529;
  text-align: center;
  white-space: nowrap;
  background-color: #e9ecef;*/
  border: 1px solid #ced4da;
  /*border-radius: 1.5rem;*/
}


.login-card-body .input-group .input-group-text-login,
.register-card-body .input-group .input-group-text-login {
  /*background-color: transparent;*/
  background-color:#fff;
  border-bottom-right-radius: 1.5rem;
  border-left: 0;
  border-top-right-radius: 1.5rem;
  color: #777;
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}


.login-card-body .input-group .form-control-login:focus ~ .input-group-append .input-group-text-login,
.register-card-body .input-group .form-control-login:focus ~ .input-group-append .input-group-text-login {
  border-color: #80bdff;
}


.login-card-body .input-group .form-control-login.is-valid ~ .input-group-append .input-group-text-login,
.register-card-body .input-group .form-control-login.is-valid ~ .input-group-append .input-group-text-login {
  border-color: #28a745;
}




